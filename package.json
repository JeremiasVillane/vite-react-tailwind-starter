{
  "name": "vite-react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && vite build",
    "build:turbo": "npx turbo build",
    "serve": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:turbo": "npx turbo lint",
    "type-check": "tsc",
    "type-check:turbo": "npx turbo type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^4.5.11",
    "@radix-ui/react-slot": "^1.0.1",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "lucide-react": "^0.216.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.11.1",
    "tailwind-merge": "^1.12.0",
    "tailwindcss-animate": "^1.0.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.1.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-helmet": "^6.1.6",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.6.3",
    "eslint": "^8.40.0",
    "eslint-config-sznm": "^1.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.2",
    "typescript": "4.9.5",
    "vite": "^4.3.5"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.1.0"
}
